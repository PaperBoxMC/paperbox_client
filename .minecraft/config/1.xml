<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
    <lines at="bottomcenter">
        <line>
           <str>{offhandlargeicon}{offhandname}$d|{mainhandlargeicon}{mainhandname}</str>
        </line>
        
        <line>
            <str>$e{offhanddamageleft}$f / $e{offhandmaxdamage}$d|$e{mainhanddamageleft}$f / $e{mainhandmaxdamage}</str>
        </line>

        <line>
            <str>$cHP:</str>
            <round>
                <var>health</var>
                <num>1</num>
            </round>
            <str>/</str>
            <round>
                <var>maxhealth</var>
                <num>1</num>
            </round>
            <operation>
            <str>GE</str>
            <var>health</var>
                <num>100</num>
                <num>80</num>
                <num>60</num>
                <num>40</num>
                <num>20</num>
                <num>17</num>
                <num>13</num>
                <num>8</num>
                <num>4</num>
                <num>0</num>
                    <str> $9(???) </str>
                    <str> $3(极限) </str>
                    <str> $d(强盛) </str>
                    <str> $a(健康) </str>
                    <str> $b(正常) </str>
                    <str> $e(微伤) </str>
                    <str> $6(受伤) </str>
                    <str> $c(重伤) </str>
                    <str> $4(垂危) </str>
            </operation>

            <operation>
            <str>GT</str>
            <var>armor</var>
                <num>59</num>
                <num>29</num>
                <num>19</num>
                <num>14</num>
                <num>9</num>
                <num>5</num>
                <num>0</num>
                    <str>$d盔甲:{armor}$d⭐⭐ </str>
                    <str>$9盔甲:{armor}$d⭐ </str>
                    <str>$a盔甲:{armor} </str>
                    <str>$e盔甲:{armor} </str>
                    <str>$6盔甲:{armor} </str>
                    <str>$c盔甲:{armor} </str>
                    <str>$4盔甲:{armor} </str>
            </operation>

            <str>$3移速:</str>
            <op>
                <str>LE</str>
                <var>speed</var>
                <num>0</num>
                <num>6</num>
                <num>10</num>
                <num>20</num>
                <num>65</num>
                <num>73</num>
                <num>145</num>
                <str></str>
                <str>$3</str>
                <str>$b</str>
                <str>$a</str>
                <str>$e</str>
                <str>$6</str>
                <str>$c</str>
                <str>$4{obfuscated}</str>
            </op>
            <round>
                <var>speed</var>
                <num>1</num>
            </round>
            <str>m/s</str>
        </line>
    </lines>

    <lines at="topleft">
        <line>
        <str> </str>
        </line>

        <line>
                <operation>
                    <str>GE</str>
                    <pct>
                        <var>memused</var>
                        <var>memmax</var>
                    </pct>
                    <num>90</num>
                    <num>80</num>
                    <num>70</num>
                    <num>0</num>
                        <str>$4$lMem: </str>
                        <str>$cMem: </str>
                        <str>$6Mem: </str>
                        <str>$aMem: </str>
                </operation>

            <round>
                <pct>
                    <var>memused</var>
                    <var>memmax</var>
                </pct>
                <num>0</num>
            </round>
            <str>% </str>

            <round>
                <mul>
                    <var>memused</var>
                    <num>0.000000001024</num>
                </mul>
                <num>1</num>
            </round>

            <str>G/</str>

            <round>
                <mul>
                    <var>memmax</var>
                    <num>0.000000001024</num>
                </mul>
                <num>1</num>
            </round>

            <str>G </str>

        <operation>
            <str>GT</str>
            <var>fps</var>
                <num>144</num>
                <num>120</num>
                <num>90</num>
                <num>75</num>
                <num>60</num>
                <num>30</num>
                <num>15</num>
                <num>0</num>
                    <str>$5FPS:{fps}</str>
                    <str>$dFPS:{fps}</str>
                    <str>$3FPS:{fps}</str>
                    <str>$bFPS:{fps}</str>
                    <str>$aFPS:{fps}</str>
                    <str>$eFPS:{fps}</str>
                    <str>$c$lFPS低:{fps}</str>
                    <str>$4$lFPS过低:{fps}</str>
        </operation>
        </line>
        <line>
           <operation>
            <str>EQ</str>
            <var>abrroughdirection</var>
                <num>E</num>
                <num>S</num>
                <num>W</num>
                <num>N</num>
                    <str>$d东 $c正X:{xi} $aY:{yi} $9Z:{zi}</str>
                    <str>$d南 $cX:{xi} $aY:{yi} $9正Z:{zi}</str>
                    <str>$d西 $c负X:{xi} $aY:{yi} $9Z:{zi}</str>
                    <str>$d北 $cX:{xi} $aY:{yi} $9负Z:{zi}</str>
            </operation>
        </line>
    </lines>
    

